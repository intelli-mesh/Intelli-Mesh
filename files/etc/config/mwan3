config member 'usb1_m2_w2'     
    option interface 'wwan'    
    option metric '1'      
    option weight '1'             
                                  
config policy 'balanced'       
#    list use_member 'usb0_m1_w2'
    list use_member 'usb1_m2_w2'
    list use_member 'wan0_m0_w0'  
    option last_resort 'unreachable'
                                 
config rule 'default_rule'       
    option dest_ip '0.0.0.0/0'  
    option proto 'all'              
    option sticky '0'               
    option use_policy 'balanced' 

