config interface 'wwan'          
    option enabled '1'          
    option initial_state 'offline'
    option family 'ipv4'            
    option reliability '2'       
    option count '1'             
    option timeout '2'          
    option interval '5'             
    option down '3'                 
    option up '3'                
    list track_ip '1.1.1.1'   
    list track_ip '1.0.0.1'       
                            
config member 'wan0_m0_w0'         
    option interface 'wan'         
    option metric '1'      
    option weight '1'             
                                  
